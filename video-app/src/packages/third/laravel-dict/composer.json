{
    "name": "third/laravel-dict",
    "description": "laravel-dict是一个用于管理系统常用的配置的简单封装.提高代码的扩展性，可以易读性",
    "license": "MIT",
    "require": {
        "php":">=5.5.0"
    },
    "require-dev" : {
         "phpunit/phpunit": "~5.0|~6.0|~7.0"
    },
    "autoload": {
        "psr-4": {"Third\\Dict\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Third\\Tests\\Dict\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Third\\Dict\\DictServiceProvider"
            ],
            "aliases": {
                "Dict": "Third\\Dict\\Facades\\Dict"
            }
        }
    }
}
